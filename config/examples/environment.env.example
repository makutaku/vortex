# Example environment variables for Vortex
# Copy to .env or set in your shell environment

# Output directory for downloaded data
VORTEX_OUTPUT_DIRECTORY="./data"

# Barchart credentials
VORTEX_BARCHART_USERNAME="your_username"
VORTEX_BARCHART_PASSWORD="your_password"

# Interactive Brokers settings
VORTEX_IBKR_HOST="localhost"
VORTEX_IBKR_PORT="7497"
VORTEX_IBKR_CLIENT_ID="1"

# Logging configuration
VORTEX_LOG_LEVEL="INFO"
VORTEX_LOG_FILE="./logs/vortex.log"