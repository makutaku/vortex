# Example Vortex configuration file
# Copy to ~/.config/vortex/config.toml and customize

output_directory = "./data"
backup_enabled = true

[providers.barchart]
username = "your_username"
password = "your_password"
daily_limit = 150

[providers.ibkr]
host = "localhost"
port = 7497
client_id = 1

[providers.yahoo]
# Yahoo Finance doesn't require credentials
timeout = 30